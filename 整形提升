int main()
{
	unsigned char a = 200;
    unsigned char b = 100;
	unsigned char c = 0;
	
	c = a + b;//整形提升
	//00000000000000000000000911001000- a
	//0000000ee0e000000000000001100100-b
	//000000ee80e000000000000100101100
	//00101100-C
	//00000000000000000000000000101100
	// 
	//11001000-a
	//00000000000000000000000001100100
	//01100100-b

	printf("%d %d", a + b, c);
	return 0;
}
